 <h1>[% Koha.Preference('RestrictedPageTitle') | html %]</h1>
 [% END %]
 [% Koha.Preference('RestrictedPageContent') | $raw %]
 </div>
 </div> <!-- / .col-10 -->
 </div> <!-- / .row -->
 </div> <!-- / .container-fluid -->
</div> <!-- / .main -->

[% INCLUDE 'opac-bottom.inc' %]
[% BLOCK jsinclude %][% END %]
