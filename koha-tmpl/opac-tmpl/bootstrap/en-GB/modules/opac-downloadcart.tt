[% INCLUDE 'doc-head-open.inc' %]
<title>Download folder &rsaquo; [% IF ( LibraryNameTitle ) %][% LibraryNameTitle | html %][% ELSE %]Koha online[% END %] catalogue</title>
[% INCLUDE 'doc-head-close.inc' %]
[% BLOCK cssinclude %][% END %]
</head>
[% INCLUDE 'bodytag.inc' bodyid='opac-downloadcart' bodyclass='popup' %]
 <div class="main">
 <div class="container-fluid">
 <div class="row">
 <div class="col order-first order-md-first order-lg-2">
 <div id="userdownloadcart" class="maincontent">
 <h1>Download folder</h1>
 <form method="post" action="/cgi-bin/koha/opac-downloadcart.pl">
 <select name="format" id="format">
 <option value="">-- Choose format --</option>
 <option value="ris">RIS (Zotero, EndNote, others)</option>
 <option value="bibtex">BibTeX</option>
 <option value="isbd">ISBD</option>
 <option value="iso2709">MARC</option>
 [% FOREACH csv_profile IN csv_profiles %]
 <option value="[% csv_profile.export_format_id | html %]">CSV - [% csv_profile.profile | html %]</option>
 [% END %]
 </select>
 <fieldset class="action">
 <input type="hidden" name="bib_list" value="[% bib_list | html %]" />
 <input type="submit" name="save" value="Go" />
 <a class="cancel close" href="#">Cancel</a>
 </fieldset>
 </form>
 <p><a class="close" href="#">Close this window</a></p>
 </div> <!-- / #userdownloadcart -->
 </div> <!-- / .span10 -->
 </div> <!-- / .row -->
 </div> <!-- / .container-fluid -->
 </div> <!-- / .main -->
[% INCLUDE 'opac-bottom.inc' is_popup=1 %]
[% BLOCK jsinclude %][% END %]
